<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\UkrLogic\TourBundle\Entity\Resort" table="resort">
    <id name="id" type="integer" column="id"/>
    <field name="name" type="string" column="name" length="255"/>
    <field name="nameEng" type="string" column="name_eng" length="255"/>
      <field name="active" type="boolean" column="active"/>
      <field name="countryName" type="string" column="countryname" length="255"/>
      <many-to-one target-entity="Application\UkrLogic\TourBundle\Entity\Country" field="country"/>
  </entity>
</doctrine-mapping>
