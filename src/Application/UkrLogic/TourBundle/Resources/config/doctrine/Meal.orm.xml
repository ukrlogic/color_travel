<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\UkrLogic\TourBundle\Entity\Meal">
    <id name="id" type="integer" column="id"/>
    <field name="name" type="string" column="name" length="255"/>
    <field name="nick" type="string" column="nick" length="255"/>
    <field name="nameEng" type="string" column="name_eng" length="255"/>
    <field name="active" type="boolean" column="active"/>
    <field name="description" type="string" column="description" length="255"/>
  </entity>
</doctrine-mapping>
