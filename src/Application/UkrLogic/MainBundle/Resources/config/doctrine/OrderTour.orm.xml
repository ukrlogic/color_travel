<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\UkrLogic\MainBundle\Entity\OrderTour">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="fio" type="string" column="fio" length="255"/>
    <field name="phone" type="string" column="phone" length="255"/>
    <field name="email" type="string" column="email" length="255"/>
    <field name="info" type="string" column="info" length="255"/>
    <field name="status" type="boolean" column="status"/>
    <field name="date" type="date" column="date"/>
  </entity>
</doctrine-mapping>
